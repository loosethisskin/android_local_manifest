    <remove-project name="Quarx2k/android_device_moto_jordan-common" />
    <project path="device/moto/jordan-common" name="defy-jb/android_device_moto_jordan-common" remote="cmdev" revision="jb-linaro" />

    <remove-project name="CyanogenMod/android_build" />
