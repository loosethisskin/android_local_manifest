<?xml version="1.0" encoding="UTF-8"?>
  <manifest>

    <remote fetch="git://cmdev/" name="cmdev" />

    <remove-project name="CyanogenMod/android" />
    <project path="android" name="Quarx2k/android" />

    <remove-project name="Quarx2k/android_vendor_cm" />
