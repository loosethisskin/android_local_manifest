<?xml version="1.0" encoding="UTF-8"?>
  <manifest>

    <!-- SGT7 Linaro Build -->

    <remote fetch="git://android.git.linaro.org/" name="linaro" />
    <remote fetch="git://git.linaro.org/" name="linaro-other" />
    <remote fetch="git://cmdev/" name="cmdev" />
    <remote name="omapzoom" fetch="git://git.omapzoom.org/" />

    <!-- CM removals -->
    <remove-project name="CyanogenMod/android_hardware_libhardware" />
    <remove-project name="CyanogenMod/android_hardware_libhardware_legacy" />
    <remove-project name="CyanogenMod/android_hardware_ti_omap4xxx" />
    <remove-project name="CyanogenMod/android_hardware_ti_wlan" />
    <remove-project name="CyanogenMod/android_system_core" />

    <!-- omapzoom replacements -->
    <project path="device/ti/common-open" name="device/ti/common-open" remote="omapzoom" revision="jb-release" />
    <project path="device/ti/proprietary-open" name="device/ti/proprietary-open" remote="omapzoom" revision="jb-release" />
    <project path="hardware/ti/domx" name="platform/hardware/ti/domx" remote="omapzoom" revision="jb-release" />
    <project path="hardware/ti/omap4xxx" name="platform/hardware/ti/omap4xxx" remote="omapzoom" revision="p-jb-release" />
    <project path="hardware/ti/wlan" name="platform/hardware/ti/wlan" remote="omapzoom" revision="p-jb-release" />
    <project path="external/wpa_supplicant_8_ti" name="platform/external/wpa_supplicant_8_ti" remote="omapzoom" revision="jb-release">
        <annotation name="vendor" value="ti" />
    </project>

    <!-- modified CM projects -->
    <project path="hardware/libhardware" name="otter/android_hardware_libhardware" remote="cmdev" revision="jb-otter" />
    <project path="hardware/libhardware_legacy" name="otter/android_hardware_libhardware_legacy" remote="cmdev" revision="jb-otter" />
    <project path="system/core" name="otter/android_system_core" remote="cmdev" revision="jb-otter" /> 

    <!-- local projects -->
    <project path="device/amazon/otter" name="otter/device-amazon-otter" remote="cmdev" revision="jb" />
    <project path="kernel/amazon/otter" name="KFire-Android/kernel_omap" revision="4AI1.7" />
    <project path="vendor/amazon/otter" name="KFire-Android/android_vendor_amazon_otter" revision="jb" />

    <remove-project name="CyanogenMod/android_vendor_cm" />
    <project path="vendor/cm" name="common/android_vendor_cm" remote="cmdev" revision="jb" />

    <remove-project name="CyanogenMod/android_build" />
    <project path="build" name="linaro/android_build" remote="cmdev" revision="jb-linaro" >
        <copyfile src="core/root.mk" dest="Makefile" />
    </project>

    <!-- SGT7 -->
    <remove-project name="CyanogenMod/android_frameworks_base" />
    <remove-project name="CyanogenMod/android_packages_apps_Email" />
    <remove-project name="CyanogenMod/android_packages_apps_Settings" />
    <remove-project name="CyanogenMod/android_packages_apps_Trebuchet" />
    <project path="frameworks/base" name="sgt7/android_frameworks_base" remote="cmdev" revision="jb-sgt7" />
    <project path="packages/apps/Email" name="sbradymobile/android_packages_apps_Email" />
    <project path="packages/apps/Settings" name="sgt7/android_packages_apps_Settings" />
    <project path="packages/apps/Trebuchet" name="sgt7/android_packages_apps_Trebuchet" />

    <!-- HWA Settings app -->
    <project path="packages/apps/HwaSettings" name="loosethisskin/HwaSettings" revision="jellybean" />

    <!-- Linaro -->
    <remove-project name="CyanogenMod/android_bionic" />
    <project path="bionic" name="linaro/android_bionic" remote="cmdev" revision="jb-linaro" />

    <!-- Linaro modifications: gcc 4.7 support -->
    <remove-project name="CyanogenMod/android_external_stlport" />
    <remove-project name="CyanogenMod/android_frameworks_av" />
    <remove-project name="CyanogenMod/android_frameworks_native" />
    <remove-project name="CyanogenMod/android_frameworks_rs" />
    <remove-project name="CyanogenMod/android_system_security" />
    <project path="external/stlport" name="linaro/android_external_stlport" remote="cmdev" revision="jb-linaro"/>
    <project path="frameworks/av" name="linaro/android_frameworks_av" remote="cmdev" revision="jb-linaro" />
    <project path="frameworks/native" name="linaro/android_frameworks_native" remote="cmdev" revision="jb-linaro" />
    <project path="frameworks/rs" name="linaro/android_frameworks_rs" remote="cmdev" revision="jb-linaro"/>
    <project path="system/security" name="linaro/android_system_security" remote="cmdev" revision="jb-linaro" />

    <!-- Linaro modifications: gcc 4.6+ -Werror support -->
    <remove-project name="CyanogenMod/android_dalvik" />
    <remove-project name="CyanogenMod/android_external_icu4c" />
    <remove-project name="CyanogenMod/android_packages_inputmethods_LatinIME" />
    <project path="dalvik" name="linaro/android_dalvik" remote="cmdev" revision="jb-linaro" />
    <project path="external/icu4c" name="linaro/android_external_icu4c" remote="cmdev" revision="jb-linaro" />
    <project path="packages/inputmethods/LatinIME" name="linaro/android_packages_inputmethods_LatinIME" remote="cmdev" revision="jb-linaro" />

    <!-- Linaro modifications: Fix the float ABI check for gcc 4.7+ -->
    <remove-project name="CyanogenMod/android_external_v8" />
    <project path="external/v8" name="linaro/android_external_v8" remote="cmdev" revision="jb-linaro" />

    <!-- Linaro modifications: Replaced traditional libjpeg with libjpeg-turbo -->
    <remove-project name="CyanogenMod/android_external_jpeg" />
    <project path="external/jpeg" name="people/tomgall/libjpeg-turbo/libjpeg-turbo" revision="android" remote="linaro-other" />

  </manifest>
