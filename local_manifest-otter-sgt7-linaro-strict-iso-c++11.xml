    <project path="external/webkit" name="linaro/android_external_webkit" remote="cmdev" revision="jb-linaro-strict-iso" />
    <project path="external/wpa_supplicant_8" name="linaro/android_external_wpa_supplicant_8" remote="cmdev" revision="jb-linaro-strict" />
    <project path="frameworks/compile/libbcc" name="linaro/android_frameworks_compile_libbcc" remote="cmdev" revision="jb-linaro-strict" />
