    <remove-project name="CyanogenMod/android_frameworks_compile_slang" />
    <remove-project name="CyanogenMod/android_frameworks_ex" />
    <remove-project name="CyanogenMod/android_frameworks_wilhelm" />
    <remove-project name="CyanogenMod/android_libcore" />
    <remove-project name="CyanogenMod/android_system_extras" />
    <project path="external/busybox" name="linaro/android_external_busybox" remote="cmdev" revision="jb-linaro-strict" />
    <project path="external/chromium" name="linaro/android_external_chromium" remote="cmdev" revision="jb-linaro-strict" />
    <project path="external/clang" name="linaro/android_external_clang" remote="cmdev" revision="jb-linaro-strict" />
    <project path="external/dnsmasq" name="linaro/android_external_dnsmasq" remote="cmdev" revision="jb-linaro-strict" />
    <project path="external/e2fsprogs" name="linaro/android_external_e2fsprogs" remote="cmdev" revision="jb-linaro-strict" />
    <project path="external/lsof" name="linaro/android_external_lsof" remote="cmdev" revision="jb-linaro-strict" />
    <project path="external/openssh" name="linaro/android_external_openssh" remote="cmdev" revision="jb-linaro-strict" />
