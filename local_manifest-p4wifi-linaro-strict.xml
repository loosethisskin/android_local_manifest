    <project path="packages/apps/Calculator" name="otter/android_packages_apps_Calculator" remote="cmdev" revision="jb-otter" />

    <!-- Linaro -->
    <remove-project name="CyanogenMod/android_bionic" />
    <project path="bionic" name="linaro/android_bionic" remote="cmdev" revision="jb-linaro" />

    <!-- Linaro modifications: gcc 4.7 support -->
    <remove-project name="CyanogenMod/android_external_stlport" />
    <remove-project name="CyanogenMod/android_frameworks_av" />
    <remove-project name="CyanogenMod/android_frameworks_native" />
    <remove-project name="CyanogenMod/android_frameworks_rs" />
