    <!-- p4wifi linaro -->

    <remote fetch="git://git.linaro.org/" name="linaro-other" />
    <remote fetch="git://cmdev/" name="cmdev" />

    <!-- local projects -->
    <project path="device/samsung/p4wifi" name="p4wifi/android_device_samsung_p4wifi" remote="cmdev" revision="jb-linaro" />
